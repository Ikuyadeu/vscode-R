<!DOCTYPE html>
<html>

<head>
    <link href="<%= asWebViewPath('style.css') %> " rel="stylesheet">
    <link href="<%= asWebViewPath('styleOverwrites.css') %> " rel="stylesheet" class="overwrites" <%- overwriteStyles ? '' : 'disabled' %>>
</head>

<body>
    <div id="largePlot" plotId=<%- largePlot?.id || '' %>>
        <%- largePlot?.svg %>
    </div>
    <div id="handler"> </div>
    <div id="smallPlots">
        <% plots.forEach((plot)=> { %>
            <div class="wrapper">
            <a
                href="<%- makeCommandUri('r.httpgd.showIndex', host, plot.id) %> "
                title="Plot <%- plot.id %>"
                class="focusPlot <%- plot.id === activePlot ? 'active' : '' %> "
                plotId="<%- plot.id %>"
            >
                <%- plot.svg %>
            </a>
            <a
                href="<%- makeCommandUri('r.httpgd.hidePlot', host, plot.id) %>"
                title="Hide Plot"
                class="hidePlot"
            >
                &#x2716;
            </a>
            </div>
        <% }) %>
    </div>
</body>
<script src="<%= asWebViewPath('index.js') %>"></script>

</html>
