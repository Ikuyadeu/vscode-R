<!DOCTYPE html>
<html>

<head>
    <link href="<%= asWebViewPath('style.css') %> " rel="stylesheet">
    <link href="<%= asWebViewPath('styleOverwrites.css') %> " rel="stylesheet" class="overwrites" <%- overwriteStyles ? '' : 'disabled' %>>
</head>

<body>
    <div id="largePlot" plotId=<%- largePlot?.id || '' %>>
        <%- largePlot?.svg %>
    </div>
    <div id="handler"> </div>
    <div id="smallPlots">
        <% plots.forEach((plot)=> { %>
            <a href="<%- makeCommandUri('r.httpgd.showIndex', host, plot.id) %> " title="Plot <%- plot.id %>">
                <div class="plotDiv <%- plot.id===activePlot ? 'active' : '' %>" plotId="<%- plot.id %>">
                    <%- plot.svg %>
                </div>
            </a>
        <% }) %>
    </div>
</body>
<script src="<%= asWebViewPath('index.js') %>"></script>

</html>
